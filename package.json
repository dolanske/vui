{
  "name": "@dolanske/vui",
  "type": "module",
  "version": "0.5.0",
  "private": false,
  "description": "Brother in Christ there's a new UI library ",
  "author": "dolanske",
  "license": "GPL-3.0",
  "sass": "src/index.scss",
  "repository": {
    "type": "git",
    "url": "https://github.com/dolanske/vui.git"
  },
  "keywords": [
    "vue",
    "vue3",
    "component"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/vui.js"
    },
    "./style": {
      "import": "./dist/style.css",
      "require": "./dist/style.css",
      "default": "./dist/style.css"
    }
  },
  "main": "./dist/vuicjs",
  "module": "./dist/vui.js",
  "browser": "./dist/vui.js",
  "unpkg": "./dist/vui.js",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist/*",
    "./src/*",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest"
  },
  "dependencies": {
    "@floating-ui/vue": "^1.1.5",
    "@types/node": "^22.9.0",
    "@vuepic/vue-datepicker": "^11.0.1",
    "@vueuse/core": "^11.2.0",
    "sass": "^1.80.6",
    "vaul-vue": "^0.2.0",
    "vite-plugin-dts": "^4.3.0",
    "vue": "^3.5.12"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.8.0",
    "@iconify/vue": "^4.1.2",
    "@vitejs/plugin-vue": "^5.1.5",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.14.0",
    "eslint-plugin-format": "^0.1.2",
    "jsdom": "^26.0.0",
    "typescript": "^5.6.3",
    "vite": "^5.4.11",
    "vitest": "^2.1.8",
    "vue-tsc": "^2.1.10"
  }
}
