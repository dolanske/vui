{
  "name": "@dolanske/vui",
  "type": "module",
  "version": "1.1.1",
  "private": false,
  "description": "Brother in Christ there's a new UI library",
  "author": "dolanske",
  "license": "GPL-3.0",
  "sass": "src/index.scss",
  "repository": {
    "type": "git",
    "url": "https://github.com/dolanske/vui.git"
  },
  "keywords": [
    "vue",
    "vue3",
    "component"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/vui.js"
    },
    "./style": {
      "import": "./dist/vui.css",
      "require": "./dist/vui.css",
      "default": "./dist/vui.css"
    }
  },
  "main": "./dist/vui.js",
  "module": "./dist/vui.js",
  "browser": "./dist/vui.js",
  "unpkg": "./dist/vui.js",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist/*",
    "./src/*",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "build:pages": "vue-tsc -b && vite build --mode pages",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest"
  },
  "dependencies": {
    "@floating-ui/vue": "^1.1.6",
    "@types/node": "^22.14.1",
    "@vuepic/vue-datepicker": "^11.0.2",
    "@vueuse/core": "^13.1.0",
    "sass": "^1.86.3",
    "vaul-vue": "^0.4.1",
    "vite-plugin-dts": "^4.5.3",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.12.0",
    "@iconify/vue": "^4.3.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.25.0",
    "eslint-plugin-format": "^1.0.1",
    "jsdom": "^26.1.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.2",
    "vitest": "^3.1.1",
    "vue-tsc": "^2.2.8"
  }
}
